{
  "name": "thinkkoa_site",
  "description": "application created by thinkkoa",
  "version": "1.0.0",
  "scripts": {
    "test": "npm run eslint",
    "start": "npm run compile && supervisor -w app -q index",
    "compile": "babel src -d app -D",
    "watch-compile": "npm run compile -- --watch",
    "eslint": "eslint src"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": true
          },
          "loose": true
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "graceful-fs": "^4.0.0",
    "highlight.js": "^8.8.0",
    "marked": "^0.3.6",
    "marked-toc": "^0.3.0",
    "minimatch": "3.0.2",
    "think_cache": "^1.x.x",
    "think_view": "^1.x.x",
    "thinkkoa": "^1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.4.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "supertest": "^3.0.0",
    "supervisor": "^0.12.0"
  }
}
